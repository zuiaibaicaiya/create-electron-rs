{
  "name": "electron-rs",
  "version": "1.0.18",
  "description": "Electron build tooling based on rsbuild",
  "author": "最爱白菜吖<1355081829@qq.com>",
  "license": "MIT",
  "homepage": "https://space.bilibili.com/388985971",
  "keywords": [
    "electron",
    "electron-rs",
    "create-electron-rs",
    "rsbuild-plugin",
    "electron-plugin",
    "plugin"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "publish:new": "npm publish --provenance false",
    "dev": "rslib build --watch",
    "format": "prettier --write .",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@rslib/core": "^0.11.0",
    "@types/node": "^22.15.30",
    "eslint": "^9.25.1",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "peerDependencies": {
    "electron": "^37.0.0",
    "bytenode": "^1.5.7"
  }
}
